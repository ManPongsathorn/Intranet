@model Intranet.ViewModels.MaintainViewModel

@{
    ViewData["Title"] = "Maintain";
}

<input type="hidden" id="useridprofile" value="@ViewBag.UserId" />

<div class="shadow-sm Maintain">
    <button class="w-100 p-3 border-0 text-start mb-0 btnMaintain d-flex justify-content-between" id="btnMaintainMenu" type="button" data-bs-toggle="collapse" data-bs-target="#collapseMaintainMenu" aria-expanded="true">
        <div>
            <i class="bi bi-file-earmark-text ms-3 me-3 btnMaintainIcon"></i>
            <span>Menu</span>
        </div>
        <div class="position-relative me-4">
            <span class="position-absolute btnArrowMaintainMenu">
                <i class="bi bi-chevron-down btnArrowMaintain"></i>
            </span>
        </div>
    </button>
    <div class="collapse" id="collapseMaintainMenu">
        <div class="row p-3">
            <div class="col">
                <button class="btn w-100 card p-3 cardMaintain  ClickMaintainMenuGroupModal" type="button" data-bs-toggle="modal" data-bs-target="#MaintainModal">
                    MenuGroup
                </button>
            </div>
            <div class="col">
                <button class="btn w-100 card p-3 cardMaintain  ClickMaintainUserMenuGroupModal" type="button" data-bs-toggle="modal" data-bs-target="#MaintainModal">
                    UserMenuGroup
                </button>
            </div>
        </div>
    </div>
</div>

<div class="shadow-sm Maintain mt-5">
    <button class="w-100 p-3 border-0 text-start mb-0 btnMaintain d-flex justify-content-between" id="btnMaintainComplaint" type="button" data-bs-toggle="collapse" data-bs-target="#collapseMaintainComplaint" aria-expanded="true">
        <div>
            <i class="bi bi-clipboard-check ms-3 me-3"></i>
            <span>Complaint</span>
        </div>
        <div class="position-relative me-4">
            <span class="position-absolute btnArrowMaintainComplaint">
                <i class="bi bi-chevron-down btnArrowMaintain"></i>
            </span>
        </div>
    </button>
    <div class="collapse" id="collapseMaintainComplaint">
        <div class="row p-3">
            <div class="col">
                <button class="btn w-100 card p-3 cardMaintain  ClickMaintainCategoryComplaintModal" type="button" data-bs-toggle="modal" data-bs-target="#MaintainModal">
                    CategoryComplaint
                </button>
            </div>
            <div class="col">
                <button class="btn w-100 card p-3 cardMaintain  ClickMaintainComplaintFromModal" type="button" data-bs-toggle="modal" data-bs-target="#MaintainModal">
                    ComplainFrom
                </button>
            </div>
        </div>
    </div>
</div>

<div class="shadow-sm Maintain mt-5">
    <button class="w-100 p-3 border-0 text-start mb-0 btnMaintain d-flex justify-content-between" id="btnMaintainLocation" type="button" data-bs-toggle="collapse" data-bs-target="#collapseMaintainLocation" aria-expanded="true">
