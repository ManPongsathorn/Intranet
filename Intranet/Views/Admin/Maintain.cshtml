@*--**********MaintainModal**********--*@
<div class="modal fade maintainmodal" id="MaintainModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
    <div class="modal-dialog modal-lg">
        @*----------MaintainMenuGroupModal----------*@
        <div class="modal-content p-3" id="MaintainMenuGroupModal">
            <div class="modal-header ps-md-3 pe-md-3">
                <label class="modal-title fw-bold titlemaintainmodal" id="MaintainModal">MenuGroup</label>
                <button type="button" class="btn closemaintainmodal position-absolute end-0 me-3 px-2 py-1" data-bs-dismiss="modal" aria-label="Close"><i class="bi bi-x-lg"></i></button>
            </div>
            <div class="modal-body" style="overflow-x:hidden;">
                <div class="row mt-3">
                    <div class="col">
                        <div class="input-group">
                            <i class="bi bi-search ps-4 position-absolute searchiconmaintain" style="z-index:2;padding-top:0.8rem;"></i>
                            <input type="text" id="SearchMaintainMenuGroupModal" class="form-control text-nowrap searchmaintain" placeholder="ค้นหากลุ่มเมนู" style="z-index: 1;border-radius: 15px;">
                        </div>

                        <div class="table-responsive p-0 mt-4" style="max-height:48vh;">
                            <table class="table table-borderless m-0 ps-2 pe-2 table tablemaintain TableMaintainMenuGroupModal">
                                <thead class="text-start align-middle">
                                    <tr>
                                        <th class="p-3"><label class="tableheadlabelmaintain fw-bold">ชื่อกลุ่มเมนู</label></th>
                                        <th class="p-3 text-center"><label class="tableheadlabelmaintain fw-bold">การจัดการ</label></th>
                                    </tr>
                                </thead>
                                <tbody class="text-start align-middle">
                                    @foreach (var item in Model.MenuGroups)
                                    {
                                        <tr>
                                            <td class="p-3"><label class="tablelabelmaintain">@item.Name</label></td>
                                            <td class="p-3 text-center">
                                                <div class="dropdown">
                                                    <button class="btn tablemorelabelmaintain" type="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="bi bi-three-dots"></i></button>
                                                    <ul class="dropdown-menu shadow-sm" style="border-radius:15px;border-color: rgba(255,255,255,0);">
                                                        <li>
                                                            <a class="dropdown-item tabledropdowneditlabelmaintain BtnEditMaintainMenuGroup" href="#" data-bs-toggle="modal" data-bs-target="#EditMaintainModal"><i class="bi bi-pencil"></i>&nbsp;&nbsp;แก้ไข</a>
                                                            <input type="hidden" id="id" value="@item.Id">
                                                            <input type="hidden" id="name" value="@item.Name">
                                                        </li>
                                                        <li>
                                                            <a class="dropdown-item tabledropdowndeletelabelmaintain BtnDeleteMaintainMenuGroup" href="#" data-bs-toggle="modal" data-bs-target="#DeleteMaintainModal"><i class="bi bi-trash"></i>&nbsp;&nbsp;ลบข้อมูล</a>
                                                            <input type="hidden" id="id" value="@item.Id">
                                                            <input type="hidden" id="name" value="@item.Name">
                                                        </li>
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

                <div class="row mt-3">
                    <div class="col">
                        <button class="btn p-2 text-nowrap btnaddmaintainmodal BtnCreateMaintainMenuGroup" type="button" data-bs-toggle="modal" data-bs-target="#CreateMaintainModal">+ เพิ่มกลุ่มเมนู</button>
                    </div>
                </div>
            </div>
        </div>
        @*----------MaintainUserMenuGroupModal----------*@
